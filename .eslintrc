{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "react-app"
   ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
      "semi": 2
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}